{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "quotemark": [true, "single"],
    "indent": [true, "spaces", 2],
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "prefer-const": false,
    "one-line": false,
    "max-line-length": false,
    "no-console": false,
    "no-shadowed-variable": false
  }
}
